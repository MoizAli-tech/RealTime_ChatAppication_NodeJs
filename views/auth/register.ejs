<section class="register flex items-center justify-center flex-col py-6 h-full">
    <div class="form-wrapper container py-2 px-6 w-1/3 rounded">
        <h1 class="md:text-xl text-lg py-6 text-center"> Be our Permanent Client</h1>
        <%if(messages){ %>
            <h1 class="text-lg text-red-500"><%= messages.error%></h1>
        <%}%>
        <form class="flex flex-col text-xs md:text-sm  " action="register" method="POST">
            <label for="email">Email</label>
            <input id="email" type="email" name="email" class="my-2 rounded"/>
            <label for="password">Password</label>
            <input  id="password" type="password" name="password" class="my-2 rounded"/>
            <label for="cpassword">Confirm Password</label>
            <input  id="cpassword" type="password" name="cpassword" class="my-2 rounded"/>
            <button class="sub-btn my-2 rounded py-1 w-20 mx-auto">Register</button>
        </form>
    </div>

   
</section>